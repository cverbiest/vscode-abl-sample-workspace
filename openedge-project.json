{
    "name": "abl-vscode-sample",
    "version": "1.0",    
    "oeversion": "12.8", 
    "graphicalMode": true, 
    "charset": "utf-8",  
    "extraParameters": "-assemblies assemblies",
    "buildDirectory": "build", 
    "buildPath": [
      { "type": "source", "path": "src" },
      { "type": "propath", "path": "${DLC}/gui/netlib/OpenEdge.net.pl" }
    ],
    "dbConnections": [ 
      {
        "name": "sports2020", 
        "connect": "-db db/sports2020 -1 -RO", 
        "schemaFile": "dump/sports2020.df",
        "aliases": []
      }
    ],
    "numThreads": 1
  }